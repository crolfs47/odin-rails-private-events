<h1>All Events</h1>

<ul>
  <% @events.each do |event| %>
  <li><%= link_to event.title, event %> - <%= event.date %>, <%= event.location %></li>
  <% end %>
</ul>


<% if current_user %>
  <%= button_to "Logout", destroy_user_session_path, method: :delete %>
<% else %>
  <%= button_to "Login", user_session_path, method: :get %>
<% end %>